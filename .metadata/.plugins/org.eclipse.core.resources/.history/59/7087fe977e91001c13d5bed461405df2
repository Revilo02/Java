package rekurzivfv;

public class recfv {
	static int fakt(int sz)
	{
		int s=sz*fakt(sz-1);
		if (sz==1)
			return sz;
		if (sz>Integer.MAX_VALUE/s)
			return 1;
			
		else 
			{
			return sz*fakt(sz-1);
			}
	}
	static public void main(String[] args)
	{
		System.out.println(fakt(10));
		System.out.print("vége");
	}
}
