package Ipar;

import java.util.Date;

//Kinyitható szék
public class Hunor extends Magor {
	private int maxLength;
	Hunor(int w, int l,int ml,String t, Date a, Gyar gy) {
		super(w, l,t,a,gy);
		maxLength=ml;
	}
	public int getMaxLength() {return maxLength;}
	public String toString() {
		
		return this.getClass().getSimpleName()+"  " +getLength()+"cm  "+ this.getMaxLength()+getWeight()+"kg    "+this.getGyar().getHelyszin()+"  "+this.getGyartido();
	}
}
