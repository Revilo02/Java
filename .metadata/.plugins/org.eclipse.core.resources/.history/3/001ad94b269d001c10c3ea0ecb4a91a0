package registry;

public class Main {
	static public void main(String[] args) {
		// 1. pelda
		Student s1 = new Student("Gipsz Jakab", "1A2B3C", 1996);
		Student s2 = new Student("Nagy Károly", "XXX111", 1998);
		Student s3 = new Student("Kis Pippin", "111XXX", 1999);

		s1.addMark(4, 2);

		Teacher t1 = new Teacher("Rend Elek", "Q1W2E3", 1973);
		Teacher t2 = new Teacher("Csirke Béla", "OKTAT6", 1980);

		System.out.println(s1); 
		System.out.println(s2);
		System.out.println(s3);

		System.out.println(t1); 
		System.out.println(t2);

		// 2. pelda 
		Course c = new Course("Zabhegyezés", "BMEVIIIZZ00", 5);
		Teacher t = new Teacher("Vastagh Béla", "VSTGBL", 1975);
		c.setTeacher(t);

		try {
		c.addStudent(new Student("Lutz Ernõ", "LTZRN0", 1997));
		c.addStudent(new Student("Szõke Barna", "BRN123", 1997));
		c.addStudent(new Student("Hervadt Virág", "HRVDTV", 1998));
		c.addStudent(new Student("Béla","BELACS",1996));
		c.addStudent(new Student("Béla","BELASZ",1995));
		c.addStudent(new Student("Béla","BELASG",1994));
		c.addStudent(new Student("Béla","BELASV",1993));
		c.addStudent(new Student("Béla","BELAST",1992));
		c.addStudent(new Student("Béla","BELAAA",1991));
		c.addStudent(new Student("Béla","BELABB",1990));
		c.addStudent(new Student("Béla","BELACC",1989));

		
		}catch( Exception e) {
			 System.err.println(e.getMessage());
			  e.printStackTrace();

		}	
		
		c.listStudents();

		try {
		c.removeByNeptun("BRN123");
		c.removeByNeptun("BRN123");
		}catch (Exception e) {
			System.err.println(e.getMessage());
			e.printStackTrace();
		}
		c.listStudents();

	}
}
